<view class='content'>
  <view class='account'>
    <view class='title'>账号</view>
    <view class='num'>
      <input placeholder='请输入用户名' bindinput='usernameInput'></input>
    </view>
  </view>
  <view class='hr'></view>
  <view class='account'>
    <view class='title'>密码</view>
    <view class='num'>
      <input placeholder='请输入密码' password='true' bindinput='passwordInput'></input>
    </view>
  </view>
  <view class='hr'></view>
</view>
<button class='btn'  bindtap='go' type='{{btnstate}}' disabled='{{disabled}}'>登录</button>
<view class='operate'>
  <view>
    <navigator url='../authorize/authorize'>微信快速登陆</navigator>
  </view>
</view>