<view class="page">

  <view style='font-size:40rpx;text-align:center;padding-top:20rpx;'>得分:{{score}}/100</view>
  <view style='border-bottom: 1rpx solid #959595;margin-top:30rpx;'></view>

  <view class='row'>
    <view data-index="0" class="{{correctResult[0]==1? 'item':'item2'}}">1</view>
    <view data-index="1" class="{{correctResult[1]==1? 'item':'item2'}}">2</view>
    <view data-index="2" class="{{correctResult[2]==1? 'item':'item2'}}">3</view>
    <view data-index="3" class="{{correctResult[3]==1? 'item':'item2'}}">4</view>
    <view data-index="4" class="{{correctResult[4]==1? 'item':'item2'}}">5</view>
  </view>

  <view class='row'>
    <view data-index="5" class="{{correctResult[5]==1? 'item':'item2'}}">6</view>
    <view data-index="6" class="{{correctResult[6]==1? 'item':'item2'}}">7</view>
    <view data-index="7" class="{{correctResult[7]==1? 'item':'item2'}}">8</view>
    <view data-index="8" class="{{correctResult[8]==1? 'item':'item2'}}">9</view>
    <view data-index="9" class="{{correctResult[9]==1? 'item':'item2'}}">10</view>
  </view>


  <view class='bottom' bindtap='goToError'>
    <view style='font-size:35rpx;'>错题已收录至错题本，点击查看！</view>
  </view>

</view>