image{
  width: 280rpx;
  height: 280rpx;
  margin: 5%;
  border-radius: 10%;
}
.module{
  width: 49%;
  display: flex;
  flex-flow: column wrap;
  align-items: center; 
  margin: 0.5%;
}
.contain{
  display: flex;
  flex-wrap: wrap;
  height: 100%;
  width: 100%;
}
.adv{
  width: 100%;
  height: 350rpx;
}
.adv image{
  width: 100%;
  height: 350rpx;
  border-radius: 0;
  margin: 0px;
}
page {
  height: 100%;
  background: linear-gradient(#52dbf3,rgb(150, 243, 212),#FFFFFF);
}